{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "trailingSlash": true,
    "routes": [
      { "src": "/admin", "dest": "/admin/index.html" },
      { "src": "/admin/(.*)", "dest": "/admin/index.html" },
      { "src": "/corporate", "dest": "/corporate/index.html" },
      { "src": "/corporate/(.*)", "dest": "/corporate/index.html" },
      { "src": "/(.*)", "dest": "/user/$1" }
    ]
  }