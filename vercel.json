{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "trailingSlash": true,
    "rewrites": [
      { "source": "/admin", "destination": "/admin/index.html" },
      { "source": "/admin/(.*)", "destination": "/admin/index.html" },
      { "source": "/corporate", "destination": "/corporate/index.html" },
      { "source": "/corporate/(.*)", "destination": "/corporate/index.html" },
      { "source": "/(.*)", "destination": "/user/index.html" }
    ]
  }